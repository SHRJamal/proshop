{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN shopping cart app with graphql",
  "main": "backend/index.ts",
  "author": "Jamal .S <me@shrjamal.com>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"pnpm backend\" \"pnpm run frontend \" ",
    "backend": "tsnd --respawn backend/index.ts",
    "frontend": "npm run dev --prefix frontend",
    "start": "ts-node backend/server.ts",
    "seed": "pnpx prisma db seed --preview-feature",
    "migrate": "pnpx prisma migrate dev",
    "studio": "pnpx prisma studio",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm i --prefix frontend && npm run build --prefix frontend"
  },
  "dependencies": {
    "@prisma/client": "^2.30.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "bcryptjs": "2.4.3",
    "colors": "1.4.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-slow-down": "^1.4.0",
    "graphql": "^15.5.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nexus": "^1.1.0",
    "typescript": "~4.4.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-slow-down": "^1.3.1",
    "@types/jsonwebtoken": "8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.7.5",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "colors": "^1.4.0",
    "concurrently": "6.2.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "prettier": "2.3.2",
    "prisma": "^2.30.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "1.1.8",
    "winston": "^3.3.3"
  }
}
